<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/rl_sd_activity" android:background="@drawable/background_lk" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="horizontal" android:id="@+id/ll_sd_head" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="@dimen/main_title_margin">
        <TextView android:textColor="@color/color_red_exit" android:id="@+id/sd_bt_em_return" android:background="@drawable/lincoln_ic_back" android:layout_width="@dimen/main_return_w" android:layout_height="@dimen/main_return_h"/>
        <TextView android:textSize="@dimen/main_title_size" android:textColor="@android:color/white" android:gravity="center_vertical" android:layout_gravity="center_vertical" android:id="@+id/sd_em_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/title_bd_swt"/>
    </LinearLayout>
    <ScrollView android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/ll_sd_head">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerHorizontal="true">
            <LinearLayout android:orientation="horizontal" android:id="@+id/ll_swv_in_all" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true">
                <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in0" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="LF_DOOR" android:drawableLeft="@drawable/indicator_speaker"/>
                    <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in2" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="RF_DOOR" android:drawableLeft="@drawable/indicator_speaker"/>
                    <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in4" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="RR_DOOR" android:drawableLeft="@drawable/indicator_speaker"/>
                    <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in6" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="LR_DOOR" android:drawableLeft="@drawable/indicator_speaker"/>
                </LinearLayout>
                <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="100px">
                    <LinearLayout android:orientation="vertical" android:id="@+id/ll_swv_in_f" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in1" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="LF_TWEETER" android:drawableLeft="@drawable/indicator_speaker"/>
                        <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in3" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="RF_TWEETER" android:drawableLeft="@drawable/indicator_speaker"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="vertical" android:id="@+id/ll_swv_in_r" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in5" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="RR_TWEETER" android:drawableLeft="@drawable/indicator_speaker"/>
                        <TextView android:textSize="35px" android:textColor="@android:color/white" android:id="@+id/tv_swv_in7" android:background="@drawable/broder_change_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="16px" android:text="LR_TWEETER" android:drawableLeft="@drawable/indicator_speaker"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:textSize="60px" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/out_speaker" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="20px" android:text="waiting for data..." android:layout_below="@+id/ll_swv_in_all" android:layout_centerHorizontal="true"/>
            <LinearLayout android:orientation="horizontal" android:id="@+id/ll_bt_onoff" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30px" android:layout_below="@+id/out_speaker" android:layout_centerHorizontal="true">
                <Button android:textSize="30px" android:textColor="@color/color_white" android:gravity="center" android:id="@+id/speaker_on" android:background="@drawable/broder_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="speaker_on"/>
                <Button android:textSize="30px" android:textColor="@color/color_white" android:id="@+id/speaker_off" android:background="@drawable/broder_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20px" android:text="speaker_off"/>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
